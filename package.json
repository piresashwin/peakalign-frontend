{
  "name": "OKR",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve dev-app --open",
    "build": "ng build o-kR --configuration production",
    "build:app": "npm run symlink:copy -- --no-watch && ng build dev-app --configuration production",
    "symlink:copy": "npm run build && symlink dist --angular --packages @peak-align/o-kR --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "proxy": "abp generate-proxy -t ng --source dev-app --target core"
  },
  "private": true,
  "dependencies": {
